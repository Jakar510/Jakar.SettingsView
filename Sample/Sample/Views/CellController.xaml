<?xml version = "1.0"
	  encoding = "UTF-8" ?>

<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 xmlns:l="clr-namespace:AiForms.Layouts;assembly=AiForms.Layouts"
			 xmlns:ef="clr-namespace:AiForms.Effects;assembly=AiForms.Effects"
			 x:Class="Sample.Views.CellController">
	<l:WrapLayout Spacing="4">
		<Label Text="Title"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="ValueText"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="Description"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="HintText"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="TitleColor"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="ValueTextColor"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="DescriptionColor"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="BackgroundColor"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="HintTextColor"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="TitleFontSize"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="ValueTextFontSize"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="DescriptionFontSize"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="IconSource"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="IconSize"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="IconRadius"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="HintFontSize"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
		<Label Text="IsEnabled"
			   ef:AddCommand.On="true"
			   ef:AddCommand.Command="{Binding CellChangeCommand}"
			   ef:AddCommand.EffectColor="#80FFFF00" />
	</l:WrapLayout>
</ContentView>
